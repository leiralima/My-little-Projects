<Window x:Name="Calculator" x:Class="Calculator1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Calculator1"
        mc:Ignorable="d"
        Title="Calculator" Height="455" Width="420" Left="10" Top="10" Background="#FF404040">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Top" Height="436">
        <TextBox TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Top" Width="361" Height="38" Margin="0,11,0,0"/>
        <Button x:Name="btnOne" Content="1" HorizontalAlignment="Left" Margin="0,54,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnTwo" Content="2" HorizontalAlignment="Left" Margin="95,54,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnThree" Content="3" HorizontalAlignment="Left" Margin="190,54,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnFour" Content="4" HorizontalAlignment="Left" Margin="0,136,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnFive" Content="5" HorizontalAlignment="Left" Margin="95,136,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnSix" Content="6" HorizontalAlignment="Left" Margin="190,136,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnSeven" Content="7" HorizontalAlignment="Left" Margin="0,216,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnEight" Content="8" HorizontalAlignment="Left" Margin="95,216,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnNine" Content="9" HorizontalAlignment="Left" Margin="190,216,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnZero" Content="0" HorizontalAlignment="Left" Margin="0,294,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnDot" Content="," HorizontalAlignment="Left" Margin="95,294,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnEquals" Content="=" HorizontalAlignment="Left" Margin="190,294,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnDiv" Content="/" HorizontalAlignment="Left" Margin="286,54,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnMulti" Content="*" HorizontalAlignment="Left" Margin="286,136,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnMinus" Content="-" HorizontalAlignment="Left" Margin="286,216,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnPlus" Content="+" HorizontalAlignment="Left" Margin="286,294,0,0" VerticalAlignment="Top" Height="65" Width="75"/>
        <Button x:Name="btnClear" Content="C" HorizontalAlignment="Left" Margin="0,373,0,0" VerticalAlignment="Top" Height="33" Width="170"/>
        <Button x:Name="btnExit" Content="Exit" HorizontalAlignment="Left" Margin="191,373,0,0" VerticalAlignment="Top" Height="33" Width="170"/>
    </Grid>
</Window>
